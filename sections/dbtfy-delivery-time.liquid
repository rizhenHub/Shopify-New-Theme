{%- if content_for_header contains 'debutify' and settings.dbtfy_delivery_time -%}
  <div class="dbtfy dbtfy-delivery_time"
       data-position="{{ section.settings.dbtfy_delivery_time_position }}"
       hidden>

    {%- for block in section.blocks -%}
      {%- for i in (1..10) -%}
        {%- capture index -%}{{ forloop.index }}{%- endcapture -%}
        {%- capture dbtfy_delivery_time_from -%}dbtfy_delivery_time_from_{{index}}{%- endcapture -%}
        {%- capture dbtfy_delivery_time_to -%}dbtfy_delivery_time_to_{{index}}{%- endcapture -%}
        {%- capture dbtfy_delivery_time_date -%}dbtfy_delivery_time_date_{{index}}{%- endcapture -%}
        {%- capture dbtfy_delivery_time_format -%}dbtfy_delivery_time_format_{{index}}{%- endcapture -%}
        {%- capture dbtfy_delivery_time_exclude_days -%}dbtfy_delivery_time_exclude_days_{{index}}{%- endcapture -%}
        {%- capture dbtfy_delivery_time_cut_off -%}dbtfy_delivery_time_cut_off_{{index}}{%- endcapture -%}
        {%- capture dbtfy_delivery_time_cut_off_time -%}dbtfy_delivery_time_cut_off_time_{{index}}{%- endcapture -%}
        {%- capture dbtfy_delivery_time_cut_off_message -%}dbtfy_delivery_time_cut_off_message_{{index}}{%- endcapture -%}
        {%- capture dbtfy_delivery_time_type -%}type_{{index}}{%- endcapture -%}
        {%- capture dbtfy_delivery_time_tag -%}tag_{{index}}{%- endcapture -%}
        {%- capture visibility -%}visibility_{{index}}{%- endcapture -%}
        {%- capture dbtfy_delivery_time_product -%}product_{{index}}{%- endcapture -%}
        {%- capture dbtfy_delivery_time_collection -%}collection_{{index}}{%- endcapture -%}
        {%- capture dbtfy_delivery_time_icon -%}dbtfy_delivery_time_icon_{{index}}{%- endcapture -%}
        {%- capture dbtfy_delivery_time_prefix -%}dbtfy_delivery_time_prefix_{{index}}{%- endcapture -%}
        {%- capture dbtfy_delivery_time_separator -%}dbtfy_delivery_time_separator_{{index}}{%- endcapture -%}
        {%- capture dbtfy_delivery_time_suffix -%}dbtfy_delivery_time_suffix_{{index}}{%- endcapture -%}

        {%- assign current_delivery_time_from = block.settings[dbtfy_delivery_time_from] -%}
        {%- assign current_delivery_time_to = block.settings[dbtfy_delivery_time_to] -%}
        {%- assign current_delivery_time_date = block.settings[dbtfy_delivery_time_date] -%}
        {%- assign current_delivery_time_format = block.settings[dbtfy_delivery_time_format] -%}
        {%- assign current_delivery_time_exclude_days = block.settings[dbtfy_delivery_time_exclude_days] -%}
        {%- assign current_delivery_time_cut_off = block.settings[dbtfy_delivery_time_cut_off] -%}
        {%- assign current_delivery_time_cut_off_time = block.settings[dbtfy_delivery_time_cut_off_time] -%}
        {%- assign current_delivery_time_cut_off_message = block.settings[dbtfy_delivery_time_cut_off_message] -%}
        {%- assign current_delivery_time_type = block.settings[dbtfy_delivery_time_type] -%}
        {%- assign current_delivery_time_tag = block.settings[dbtfy_delivery_time_tag] -%}
        {%- assign current_visibility = block.settings[visibility] -%}
        {%- assign current_delivery_time_product = block.settings[dbtfy_delivery_time_product] -%}
        {%- assign current_delivery_time_collection = block.settings[dbtfy_delivery_time_collection] -%}
        {%- assign current_delivery_time_icon = block.settings[dbtfy_delivery_time_icon] -%}
        {%- assign current_delivery_time_prefix = block.settings[dbtfy_delivery_time_prefix] -%}
        {%- assign current_delivery_time_separator = block.settings[dbtfy_delivery_time_separator] -%}
        {%- assign current_delivery_time_suffix = block.settings[dbtfy_delivery_time_suffix] -%}

        <div class="dt-info"
            data-min-date="{{ current_delivery_time_from }}"
            data-max-date="{{ current_delivery_time_to }}"
            data-show-as-date="{{ current_delivery_time_date }}"
            data-time-format="{{ current_delivery_time_format }}"
            data-exclude-days="{{ current_delivery_time_exclude_days }}"
            data-show-cut-off="{{ current_delivery_time_cut_off }}"
            data-cut-off-time="{{ current_delivery_time_cut_off_time }}"
            data-cut-off-message="{{ current_delivery_time_cut_off_message }}"
            data-product-types="{{ current_delivery_time_type }}"
            data-product-tags="{{ current_delivery_time_tag }}"
            data-time-zone="{{ settings.time_zone }}"
            data-visibility-type="{{ current_visibility }}"
            data-product="{{ current_delivery_time_product.id }}"
            data-product-collection="{{ collections[current_delivery_time_collection].handle }}">

          {%- unless current_delivery_time_icon == blank -%}
            <span class="{{ settings.icon }} icon-width">{{ current_delivery_time_icon }}</span>
          {% endunless %}

          <span class="delivery_time_prefix">{{ current_delivery_time_prefix }}</span>
          <strong class="from-text-delivery_time"></strong>
          <span class="delivery_time_separator">{{ current_delivery_time_separator }}</span>
          <strong class="to-text-delivery_time"></strong>
          <span class="delivery_time_sufix">{{ current_delivery_time_suffix }}</span>

          {%- if current_delivery_time_cut_off -%}
            <span class="delivery_cut_off">
              (<span class="delivery_cut_off_message">
                {{- current_delivery_time_cut_off_message | append: ' ' -}}
              </span>
              <span class="delivery_cut_off_time">
                {{- current_delivery_time_cut_off_time -}}
              </span>)
            </span>
          {%- endif -%}
        </div>
      {%- endfor -%}
    {%- endfor -%}
  </div>
{%- endif -%}

